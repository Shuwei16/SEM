<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>GG</title>
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@5.2.3/dist/css/bootstrap.min.css" integrity="sha384-rbsA2VBKQhggwzxH7pPCaAqO46MgnOM80zW1RWuH61DGLwZJEdK2Kadq2F9CUG65" crossorigin="anonymous">
</head>
<body>
    <table class="table table-striped-columns">
        <thead>
            <th scope="col">Programme ID</th>
            <th scope="col">Programme Name</th>
            <th scope="col">Programme Level</th>
            <th scope="col">Price (RM)</th>
            <th scope="col">Description</th>
        </thead>
        <tbody>
            <%progList.forEach( prog => { %>
                <tr>
                <td scope="col"><%= prog.progID %></td>
                <td scope="col"><%= prog.name %></td>
                <td scope="col"><%= prog.level %></td>
                <td scope="col"><%= prog.price %></td>
                <td scope="col"><%= prog.description %></td>
                </tr>
            <% })  %>
        </tbody>
    </table>
    
    <br/>

    <table class="table table-dark table-striped">
        <thead>
            <th scope="col">Programme ID</th>
            <th scope="col">Programme Name</th>
            <th scope="col">Programme Level</th>
            <th scope="col">Price (RM)</th>
            <th scope="col">Description</th>
        </thead>
        <tbody>
            <%progList.forEach( prog => { %>
                <tr>
                <td scope="col"><%= prog.progID %></td>
                <td scope="col"><%= prog.name %></td>
                <td scope="col"><%= prog.level %></td>
                <td scope="col"><%= prog.price %></td>
                <td scope="col"><%= prog.description %></td>
                </tr>
            <% })  %>
        </tbody>
    </table>
    <p>progList.</p>
</body>
</html>